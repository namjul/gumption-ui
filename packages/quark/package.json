{
  "name": "@gumption-ui/quark",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "main": "dist/esm",
  "scripts": {
    "build": "tsc --outDir dist/esm --declaration true",
    "clean": "echo \"not specified\"",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "yarn lint:types & yarn lint:eslint",
    "lint:eslint": "eslint ./src",
    "lint:types": "tsc --noErrorTruncation --noEmit ",
    "start": "nodemon --exec yarn build -e ts,tsx --ignore dist/"
  },
  "dependencies": {
    "@emotion/css": "^11.0.0-next.12",
    "@types/react": "^16.9.34",
    "reakit-system": "^0.12.1"
  },
  "peerDependencies": {
    "react": "16.x"
  }
}
