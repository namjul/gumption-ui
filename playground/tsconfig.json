{
  "extends": "../tsconfig.base.json",
  "include": ["src", "*.d.ts"],
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "noEmit": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": false
  }
}
